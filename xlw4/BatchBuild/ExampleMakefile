
ifeq ($(DEV),devcpp)
makefile=Makefile.win
target=clean all
else
makefile=Makefile
target=rebuild
endif 


.PHONY : all


all:  
	cd ..\\xlw\\examples\\Example\\$(DEV)  && $(MAKE) -f $(makefile) BUILD=DEBUG $(target) >> ..\\..\\..\\..\\BatchBuild\\ExampleAutoGenerated_$(DEV)_Debug.log 2>&1  
	cd ..\\xlw\\examples\\Example\\$(DEV)  && $(MAKE) -f $(makefile) BUILD=RELEASE $(target) >> ..\\..\\..\\..\\BatchBuild\\ExampleAutoGenerated_$(DEV)_Release.log 2>&1  
	